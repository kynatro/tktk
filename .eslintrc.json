{
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-unused-vars": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  }
}
